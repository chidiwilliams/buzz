  "files.associations": {
    ".coveragerc": "ini",
    "Buzz.spec": "python",
    "iosfwd": "cpp"
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true
